{
  "name": "Typed URL History",
  "version": "1.2",
  "description": "Reads your history, and shows the top ten pages you go to by typing the URL.",
  "permissions": [
    "tabs",
    "history",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "clock.png"
  },
  "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*", "file://*/*" ],
      "run_at": "document_end"
   } ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "manifest_version": 2
}
